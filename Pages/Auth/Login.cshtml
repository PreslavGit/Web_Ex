@page
@model webex.Pages.LoginModel
@{
    ViewData["Title"] = "Log in";
}

<h1 class="text-white">@ViewData["Title"]</h1>
<div class="row">
    <div class="col-md-4 text-neutral">
        <form method="post">
            <div class="form-group">
                <label asp-for="Input.Email"></label>
                <input asp-for="Input.Email" class="form-control" value="admin@mail.com" />
                <span asp-validation-for="Input.Email"></span>
            </div>
            <div class="form-group">
                <label asp-for="Input.Password"></label>
                <input asp-for="Input.Password" class="form-control" value="taina_parola_za_ADMIN1" />
                <span asp-validation-for="Input.Password"></span>
            </div>
            <button type="submit" class="btn btn-primary m-0">Log in</button>
        </form>
    </div>
</div>
